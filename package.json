{
  "name": "application-metrics",
  "version": "1.2.2",
  "type": "module",
  "description": "Metrics for a NodeJS application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prebuild": "npm run clean && npm run lint && npm test",
    "test": "jest",
    "compile": "tsc",
    "rollup": "npx rollup dist/index.js --file dist/index.cjs --format cjs",
    "build": "npm run compile && npm run rollup"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papirosko/application-metrics.git"
  },
  "keywords": [
    "prometheus",
    "metrics"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/papirosko/application-metrics/issues"
  },
  "homepage": "https://github.com/papirosko/application-metrics#readme",
  "devDependencies": {
    "@types/node": "^12.20.37",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "29.7.0",
    "ts-jest": "29.2.4",
    "jest-junit": "^16.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "prom-client": "^15.1.3",
    "rollup": "^4.26.0",
    "scats": "^1.4.2",
    "tslib": "^2.1.0"
  }
}
